<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="applications-menu" label="Applications" execute="/usr/share/openbox/xdg-menu applications"/>
<menu id="preferences-menu" label="Preferences" execute="/usr/share/openbox/xdg-menu preferences"/>
<menu id="administration-menu" label="Administration" execute="/usr/share/openbox/xdg-menu system-settings"/>
<menu id="terminals-menu" label="Terminals" execute="/usr/share/openbox/xdg-menu /usr/share/openbox/terminals"/>

<menu id="root-menu" label="Openbox 3">
  <separator/>
  <item label="demos">
    <action name="Execute" >
	<execute>
		app
	</execute>
    </action>
  </item>
  <separator/>
  <item label="terminal">
    <action name="Execute" >
	<execute>
		gnome-terminal
	</execute>
    </action>
  </item>
  <separator/>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Exit">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
  <separator/>
  <item label="Log Out">
    <action name="SessionLogout">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
